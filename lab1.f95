		PROGRAM MAIN
			CHARACTER(LEN=66) :: 	INPUT_STRING
			CHARACTER*2				COMMAND
			INTEGER					IDNUM
			CHARACTER*4				DEPT
			CHARACTER*9				RANK
			REAL					PAY

		OPEN(5, STATUS="OLD", FILE="lab1data.txt")
		OPEN(6, FILE="lab1ANS.txt")

		WRITE(6, *) "*** PROGRAM STARTED ***"
	50	READ(5, 100, END=999) INPUT_STRING
	100		FORMAT(A66)

			READ(INPUT_STRING, 200) COMMAND
	200		FORMAT(A2)

			WRITE(*, *) "COMMAND = ", COMMAND

			IF (COMMAND == "IN") THEN
				CALL INSERT_OP(INPUT_STRING)
			ELSE IF (COMMAND == "DE") THEN
				CALL DELETE_OP(INPUT_STRING)
			ELSE IF (COMMAND == "PA") THEN
				CALL PRINTALL_OP()
			END IF

		GO TO 50


	999	WRITE (6, *) "END OF DATA REACHED"
		END


		

		SUBROUTINE INSERT_OP(STR)
			CHARACTER*66 	STR
			INTEGER			IDNUM
			CHARACTER*13	NAME
			CHARACTER*4		DEPT
			CHARACTER*9		RANK
			REAL			PAY

			READ(STR, 300) IDNUM, NAME, DEPT, RANK, PAY
	300		FORMAT(3X, I8, 1X, A13, 1X, A4, 11X, A9, 8X, F9.2)
			WRITE(6, *) "** In INSERT_OP w/ ID = ", IDNUM, " NAME = ", NAME, " **"
			RETURN
		END

		

		SUBROUTINE DELETE_OP(STR)
			CHARACTER*66 	STR
			INTEGER 		IDNUM

			READ(STR, 400) IDNUM
	400		FORMAT(3X, I8)
			WRITE(6, *) "** In DELETE_OP w/ ID = ", IDNUM, " **"
			RETURN
		END



		SUBROUTINE PRINTALL_OP()
			WRITE(6, *) "** In PRINTALL_OP w/ NO ADDITIONAL INPUT DATA **"
			RETURN
		END

